.DS_Store

# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
/public/*
/tmp/*
/tmp
/vendor/*
log/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development.
/storage/*
!/storage/.keep
.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore dotenv file
.env
.history

# Docker Stuff
docker-compose.override.yml

# SimpleCov
/coverage

# Jet Brain
.idea/
*.iml
*.ipr
*.iws

# Dependency directories
node_modules/

coverage/.resultset.json
coverage/.last_run.json
log/development.log
log/test.log
tmp/pids/server.pid
